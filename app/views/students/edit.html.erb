<h1>学生情報編集</h1>
<%= form_with(model:@student,url:{
    controller: "students",action:"update",id:@student.id}) do |f| %>
    <div>
        <%= f.label(:name,"名前") %>
        <%= f.text_field(:name) %> 
    </div>
    <div>
        <%= f.label(:age,"年齢") %>
        <%= f.text_field(:age) %> 
    </div>
    <%= f.submit("更新") %>
<% end %>