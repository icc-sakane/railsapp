<h1>学生情報登録</h1>
<%= form_with(model:@student,
            url:{controller: "students",action:"create"}) do |f| %>
    <div>
        <%= f.label(:name,"名前") %>
        <%= f.text_field(:name) %> 
    </div>
    <div>
        <%= f.label(:age,"年齢") %>
        <%= f.text_field(:age) %> 
    </div>
    <%= f.submit("登録") %>
<% end %>

    